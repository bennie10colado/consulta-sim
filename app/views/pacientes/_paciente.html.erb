<div id="<%= dom_id paciente %>">
  <p>
    <strong>Nome completo:</strong>
    <%= paciente.nome_completo %>
  </p>

  <p>
    <strong>Data nascimento:</strong>
    <%= paciente.data_nascimento.strftime("%d/%m/%Y") %>
  </p>

  <p>
    <strong>CPF:</strong>
    <%= paciente.CPF %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= paciente.email %>
  </p>

</div>
